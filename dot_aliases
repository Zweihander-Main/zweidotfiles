alias yt='youtube-dl'
alias ytmp4='youtube-dl -i -f mp4'
alias start='cmd.exe /c start ""'
alias ohmyzshupdate='cd;curl -s -L -o oh-my-zsh-master.tar.gz https://github.com/robbyrussell/oh-my-zsh/archive/master.tar.gz;chezmoi import --strip-components 1 --destination ${HOME}/.oh-my-zsh oh-my-zsh-master.tar.gz;compaudit | xargs chmod g-w,o-w'
