{
  "telemetry.telemetryLevel": "off",
  "emmet.triggerExpansionOnTab": true,
  "editor.fontFamily": "Source Code Pro, Consolas, 'Courier New', monospace",
  "editor.insertSpaces": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.rulers": [80],
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "files.trimTrailingWhitespace": true,
  "window.zoomLevel": 0,
  "editor.minimap.showSlider": "always",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.enabled": true,
  "editor.fontSize": 15,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Monokai Night",
  "workbench.fontAliasing": "antialiased",
  "git.autofetch": true,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  "todo-tree.tree.showScanModeButton": false,
  "cSpell.logLevel": "Information",
  "postcssSorting.config": {
    "properties-order": "alphabetical"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.inlineValues": true,
  "editor.suggest.showStatusBar": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "%"],
      "commands": ["editor.emmet.action.matchTag"]
    },
    {
      "before": ["<leader>", "b", "k"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "c", "D"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "c", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "S"],
      "commands": ["git.stage"]
    },
    {
      "before": ["<leader>", "s", "l"],
      "commands": ["editor.action.openLink"]
    },
    {
      "before": ["<leader>", "w", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<leader>", "w", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["g", "s", "<leader>"],
      "after": ["<leader>", "<leader>", "/"]
    },
    {
      "before": ["g", "s"],
      "after": ["<leader>", "<leader>"]
    }
  ],
  "workbench.editor.wrapTabs": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "sync.gist": "3f20f9eae18b93cc2fee68e0f1ba08f5",
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "editor.bracketPairColorization.enabled": false,
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#303030",
    "tab.activeBorder": "#32A8E3",
    "tab.hoverBackground": "#161616",
    "tab.border": "#303030"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "NEXT"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "window.menuBarVisibility": "toggle",
  "inline-parameters.enabled": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules",
    "/node_modules",
    "node_modules"
  ],
  "redhat.telemetry.enabled": false,
  "python.languageServer": "Pylance",
  "stylelint.validate": ["css", "less", "postcss", "scss"],
