super + alt + l
    loginctl lock-session

ctrl + alt + shift + 2
    toggle_fsearch

super + Escape
    pkill sxhkd && sxhkd &

XF86AudioLowerVolume
    pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioRaiseVolume
    pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioPlay
    playerctl play-pause

XF86AudioStop
    playerctl stop

XF86AudioPrev
    playerctl previous

XF86AudioNext
    playerctl next
